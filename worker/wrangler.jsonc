{
  "name": "claude-sandbox-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "containers": [
    {
      "class_name": "ClaudeContainer",
      "image": "./Dockerfile",
      "instance_type": "standard-1",
      "max_instances": 10
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "ClaudeContainer",
        "name": "ClaudeContainer"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["Sandbox"],
      "tag": "v1"
    },
    {
      "deleted_classes": ["Sandbox"],
      "new_sqlite_classes": ["ClaudeContainer"],
      "tag": "v2"
    }
  ]
}
